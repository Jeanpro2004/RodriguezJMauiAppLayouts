<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:TuNombreDeProyecto"
    x:Class="TuNombreDeProyecto.AppShell">

	<ShellContent Title="Grid" ContentTemplate="{DataTemplate local:GridPage}" Route="GridPage"/>
	<ShellContent Title="Stack" ContentTemplate="{DataTemplate local:StackPage}" Route="StackPage"/>
	<ShellContent Title="Flex" ContentTemplate="{DataTemplate local:FlexPage}" Route="FlexPage"/>
	<ShellContent Title="Absolute" ContentTemplate="{DataTemplate local:AbsolutePage}" Route="AbsolutePage"/>
</Shell>

